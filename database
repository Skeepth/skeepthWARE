			v4[v5] = v6;
		end;
		if not p2 then
			v3 = v4;
			return v3;
		end;
	else
		v4 = p1;
		v3 = v4;
		return v3;
	end;
	for v7, v8 in pairs(p2) do
		if v7:sub(1, 1) == "_" then
			v4[v7:sub(2)] = nil;
		else
			v4[v7] = v8;
		end;
	end;
	return v4;
end;
